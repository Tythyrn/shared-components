{
    "presets": [
        "@babel/preset-env",
        ["@babel/preset-react", {
          "runtime": "automatic",
          "jsxImportSource": "preact" // Specify the source for JSX transformation
        }]
      ],
      "plugins": [
        ["@babel/plugin-transform-react-jsx", {
          "pragma": "h",
          "pragmaFrag": "Fragment"
        }]
      ]
}